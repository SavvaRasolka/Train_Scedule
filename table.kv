
#:import Factory kivy.factory.Factory

<SearchMenu@Popup>:
    background_color: (.61, .87, .95, 1)
    background_normal: ''
    auto_dismiss: False
    title: "Поиск данных"
    size_hint: .7 , .65
    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        Label:
            text: 'Выберите условие поиска'
            size_hint_y: None
            height: 60
            font_size: 20
        BoxLayout:
            orientation: 'vertical'
            padding: 100, 0
            Button:
                text: 'По номеру поезда'
                on_press: Factory.SearchNumTrain().open()
                on_release: root.dismiss()
            Button:
                text: 'По времени отправления'
                on_press: Factory.SearchTimeDep().open()
                on_release: root.dismiss()
            Button:
                text: 'По времени прибытия'
                on_press: Factory.SearchTimeArr().open()
                on_release: root.dismiss()
            Button:
                text: 'По станции отправления'
                on_press: Factory.SearchStDep().open()
                on_release: root.dismiss()
            Button:
                text: 'По станции прибытия'
                on_press: Factory.SearchStArr().open()
                on_release: root.dismiss()
            Button:
                text: 'По времени в пути'
                on_press: Factory.SearchTravelTime().open()
                on_release: root.dismiss()
        Button:
            pos_hint: {'right':0.65}
            size_hint: 0.3, None
            height: 50
            text: 'Отмена'
            on_press: root.dismiss()



<SearchStDep@Popup>:
