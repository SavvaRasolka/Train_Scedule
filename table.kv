<AddPopup@Popup>:
    background_color: (.61, .87, .95, 1)
    background_normal: ''
    auto_dismiss: False
    title: "Добавление данных"
    size_hint: .7 , .75
    BoxLayout:
        orientation: 'vertical'
        padding: 0, 0, 0, 10
        Label:
            color: 1, 1, 1, 1
            text: 'Введите данные'
            font_size: '20'
            size_hint_y: None
            height: 70
        GridLayout:
            cols: 2
            padding: 100, 0, 120 ,15
            spacing: 10, 10
            Label:
                color: 1, 1, 1, 1
                text: 'Номер поезда:'
                size_hint_x: 0.4
            TextInput:
                multiline: False
                id: train_num
            Label:
                color: 1, 1, 1, 1
                text: 'Станция отправления:'
                size_hint_x: 0.5
            TextInput:
                multiline: False
                id: station_from
            Label:
                color: 1, 1, 1, 1
                text: 'Станция прибытия:'
                size_hint_x: 0.4
            TextInput:
                multiline: False
                id: station_to
            Label:
                color: 1, 1, 1, 1
                text: 'Дата отправления:'
                size_hint_x: 0.5
            BoxLayout:
                Label:
                    color: 0, 0, 0, 1
                    canvas.before:
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    id: dep_d
                Button:
                    text: 'Выбрать дату'
                    on_press: app.view.show_date_picker()
                    on_release: app.view.buffer = dep_d
            Label:
                color: 1, 1, 1, 1
                text: 'Время отправления:'
                size_hint_x: 0.5
            BoxLayout:
                Label:
                    color: 0, 0, 0, 1
                    canvas.before:
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    id: dep_t
                Button:
                    text: 'Выбрать время'
                    on_press: app.view.show_time_picker()
                    on_release: app.view.buffer = dep_t
            Label:
                color: 1, 1, 1, 1
                text: 'Дата прибытия:'
                size_hint_x: 0.5
            BoxLayout:
                Label:
                    color: 0, 0, 0, 1
                    canvas.before:
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    id: arr_d
                Button:
                    text: 'Выбрать дату'
                    on_press: app.view.show_date_picker()
                    on_release: app.view.buffer = arr_d

            Label:
                color: 1, 1, 1, 1
                text: 'Время прибытия:'
                size_hint_x: 0.4
            BoxLayout:
                Label:
                    color: 0, 0, 0, 1
                    canvas.before:
                        Rectangle:
                            size: self.size
                            pos: self.pos
                    id: arr_t
                Button:
                    text: 'Выбрать время'
                    on_press: app.view.show_time_picker()
                    on_release: app.view.buffer = arr_t

        Button:
            text: 'Добавить'
            size_hint: 0.4 , None
            height: 40
            pos_hint: {'right':0.7}
            on_press: app.controller.add_note(root, train_num, station_from, station_to, dep_d, dep_t, arr_d, arr_t)
        Button:
            text: 'Отмена'
            on_press: root.dismiss()
            pos_hint: {'right':0.7}
            size_hint: 0.4, None
            height: 40

